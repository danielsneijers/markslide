[ignore]
.*/__tests__/.*
.*/node_modules/stylelint/.*
.*/node_modules/prettier/.*

[include]
<PROJECT_ROOT>/src/

[libs]

[options]
module.system.node.resolve_dirname=src
module.name_mapper='^config\/\(.*\)$' -> '<PROJECT_ROOT>/config/\1'
