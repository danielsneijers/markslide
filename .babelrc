{
  "env": {
    "development": {
      "presets": [
        [
          "env", {
            "modules": false,
            "targets": { "browsers": ["last 2 versions", "> 5%"] }
          }
        ],
        "react"
      ],
      "plugins": [
        "react-hot-loader/babel"
      ]
    },
    "test": {
      "presets": [
        [
          "env", {
            "targets": { "browsers": ["last 2 versions", "> 5%"] }
          }
        ],
        "react"
      ]
    }
  }
}
